    var serviceUrl;
    //海外注册地址拼接的/account/acaslogin需改成/account/caslogin
    serviceUrl = encodeURIComponent(config.domainMain[locale] + "/account/caslogin?url=" + encodeURIComponent(window.location.href));


    // 登录
    config.loginLink[locale] = config.domainMain[locale] + '/account/login?url=' + encodeURIComponent(window.location.href);
    // 注册
    config.registerLink[locale] = config.registerUrl[locale] + '&service=' + serviceUrl;

    // 已跟亲选站点确认亲选的登录/注册是同一个地址,亲选劫持到这个地址进行登录/注册后帮忙跳转回来
    if (locale == 'zh-CNQX') {
        config.registerLink[locale] = config.loginLink[locale];
    }

    for (x in config) {
        window[x] = config[x][locale];
    }

     
    //shopdcportal-dev.test.vmall.com   pic9-dev.test.vmall.com  weixinactivity.vmall.com saleshopdc.test.vmall.com
    function checkIsDev() {
        return window.location.host.indexOf("dev9.test.vmall.com") > -1 || window.location.host.indexOf("dev.test.vmall.com") > -1 || window.location.host.indexOf("saleshopdc.test.vmall.com") > -1 || window.location.host.indexOf("weixinactivity.vmall.com") > -1;
    }

    function checkIsSt() {
        return window.location.host.indexOf("st.test.vmall.com") > -1
    }
    if (checkIsDev()) { //只要是开发环境相关域名配置指向中文
        window.domainMain = "https://webdev.test.vmall.com";
        window.amsDomain = "https://ams-kf.test.vmall.com";
        window.openapiDomain = "https://openapidev.test.vmall.com";
        window.dapDomain = 'https://dapdev.test.vmall.com';
    } else if (checkIsSt()) { //ST环境需要单独配置
        window.domainMain = "https://webst.test.vmall.com";
        window.amsDomain = "https://amsfront-st.test.vmall.com";
        window.openapiDomain = "https://openapist.test.vmall.com";
        window.dapDomain = 'https://dapdev.test.vmall.com';
    }

    delete window.registerUrl;
})(window);